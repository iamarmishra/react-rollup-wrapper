{
    "name": "react-rollup-wrapper",
    "version": "1.0.0",
    "description": "React rollup configuration for webiny/ui project",
    "main": "dist/dex.bundle.cjs.js",
    "module": "dist/dex.bundle.es.js",
    "scripts": {
        "build": "rimraf dist/* && rollup -c",
        "watch": "chokidar \"src/**\" -c \"npm run build\""
