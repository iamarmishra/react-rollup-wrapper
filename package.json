    "license": "ISC",
    "dependencies": {
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-proptypes": "^1.0.0"
    },
    "peerDependencies": {
        "@webiny/ui": "4.14.0"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@babel/preset-react": "^7.12.13",
        "@webiny/ui": "^4.14.0",
        "postcss": "^8.2.6",
        "rollup": "^2.40.0",
        "rollup-plugin-babel": "^4.4.0",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-node-resolve": "^5.2.0",
        "rollup-plugin-postcss": "^4.0.0",
        "rollup-plugin-uglify": "^6.0.4",
        "chokidar-cli": "^2.1.0"
    },
    "files": [
        "dist"
    ]
